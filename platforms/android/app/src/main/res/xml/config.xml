<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="200005" id="au.com.seoinstitute.apbi.inspectorapp" version="2.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <name>APBIInspector</name>
    <description>
        SEO Institute of Australia
    </description>
    <author email="info@seoinstitute.com.au" href="http://seoinstitute.com.au">
        SEO Institute of Australia
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/mipmap-hdpi/ic_launcher.png" />
    <icon density="mdpi" src="res/icon/android/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="res/icon/android/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />
    <splash density="land-hdpi" src="res/screen/android/drawable-land-hdpi/screen.png" />
    <splash density="land-ldpi" src="res/screen/android/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="res/screen/android/drawable-land-mdpi/screen.png" />
    <splash density="land-xhdpi" src="res/screen/android/drawable-land-xhdpi/screen.png" />
    <splash density="port-hdpi" src="res/screen/android/drawable-port-hdpi/screen.png" />
    <splash density="port-ldpi" src="res/screen/android/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="res/screen/android/drawable-port-mdpi/screen.png" />
    <splash density="port-xhdpi" src="res/screen/android/drawable-port-xhdpi/screen.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
